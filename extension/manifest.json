{
    "manifest_version": 2,
    "name": "Fingerprinting Detector",
    "version": "1.0",
    "description": "Detects fingerprinting scripts on webpages",
    "permissions": ["webRequest", "webRequestBlocking", "<all_urls>", "activeTab"],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        }
    }
}
